object jets : JET
   select  {JET}Pt > 50 AND abs({JET }Eta) < 4

define ahz = jets[-1] jets[-2]

object hZs : COMB( jets[-1] jets[-2] ) alias  
     select {jets[-1] }Pt > 20.1
     select {jets[-2] }Pt > 30.1
     select { ahz }m [] 50 150

region test
select ALL       # to count all events
select Size(jets) > 2
select Size(hZs) > 0
