object jets : JET
   select  {JET}Pt > 50 AND abs({JET }Eta) < 4

object hZs : COMB( jets[-1] jets[-2] ) alias ahz 
     select { ahz }m < 200
     select {jets[-1] }Pt > 20.1
     select {jets[-2] }Pt > 90.1
     select {jets[-2], ahz }dR > 0.2
     select { ahz }AbsEta < 2.07

region test
	select ALL       # to count all events
	select Size(jets) > 2
	select Size(hZs) > 1
	select  {hZs[0] }Pt > 100
	select  {hZs[0], hZs[1] }dR > 0.5

